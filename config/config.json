{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "application-portal-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/PortalApplicationCustomizer.js",
          "manifest": "./src/extensions/PortalApplicationCustomizer.manifest.json"
        }
      ]
    },
    "search-configuration-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/searchConfiguration/SearchConfigurationWebPart.js",
          "manifest": "./src/webparts/searchConfiguration/SearchConfigurationWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "PortalApplicationCustomizerStrings": "lib/extensions/loc/{locale}.js",
    "SearchConfigurationWebPartStrings": "lib/webparts/searchConfiguration/loc/{locale}.js"
  }
}
